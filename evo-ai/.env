# .env.example for Evo AI stack

# Domain configuration
DOMAIN=seudominio.com.br
SUBDOMAIN_EVOAPI=api
SUBDOMAIN_EVOFRONT=front

# API metadata (optional overrides)
API_TITLE=Evo API
API_DESCRIPTION=API para execução de agentes de IA
API_VERSION=1.0.0

# Database (Postgres)
POSTGRES_PASSWORD=postgres

# Cache (Redis)
REDIS_PASSWORD=

# JSON Web Tokens (JWT)
JWT_SECRET_KEY=

# Encryption
ENCRYPTION_KEY=
SECRET_KEY=

# Admin & Demo credentials
ADMIN_EMAIL=adm@example.com
ADMIN_INITIAL_PASSWORD=SuaSenhaAdm
DEMO_EMAIL=demo@example.com
DEMO_PASSWORD=demo123
DEMO_CLIENT_NAME=DemoClient

# Email / SMTP configuration
EMAIL_PROVIDER=smtp
SMTP_HOST=smtp.gmail.com
SMTP_FROM=seuemail@gmail.com
SMTP_USER=seuemail@gmail.com
SMTP_PASSWORD=suasenhadeapp
SMTP_PORT=587
SMTP_USE_TLS=true
SMTP_USE_SSL=false 